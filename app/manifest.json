{
  "name": "Example",
  "version": "0.1.0",
  "manifest_version" : 2,
  "description": "Nula",
  "browser_action" : {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "identity",
    "*://www.youtube.com/*",
    "webRequest",
    "webRequestBlocking",
    "http://www.radionula.com/",
    "https://accounts.google.com/o/oauth2/token"
  ],
  "oauth2": {
    "client_id": "{{oauthClientId}}",
    "scopes": [
      "https://www.googleapis.com/auth/youtube"
    ]
  },
  "background": {
    "scripts": [
      "background/audio.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://apis.google.com https://www.youtube.com https://s.ytimg.com; object-src 'self'"
}
